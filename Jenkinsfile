pipeline {
    agent any
    stages {
        
        stage ('Compile Stage') {
            steps {
                echo "This is compile stage"
            }
        }

        stage ('Testing Stage') {
            steps {
                echo "This is test stage"
            }
        }


        stage ('Deployment Stage') {
            steps {
                echo "This is deployment stage"
            }
        }
    }
}